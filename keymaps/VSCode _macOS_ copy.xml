<keymap version="1" name="VSCode (macOS) copy" parent="VSCode OSX">
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="shift meta e" />
    <keyboard-shortcut first-keystroke="meta 4" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="f12" />
    <mouse-shortcut keystroke="meta button1" />
    <mouse-shortcut keystroke="button2" />
    <mouse-shortcut keystroke="Force touch" />
    <keyboard-shortcut first-keystroke="meta number_sign" />
  </action>
  <action id="Javascript.Linters.EsLint.Fix">
    <keyboard-shortcut first-keystroke="ctrl meta back_quote" second-keystroke="meta f" />
  </action>
  <action id="NextProjectWindow">
    <keyboard-shortcut first-keystroke="meta back_quote" />
  </action>
  <action id="NextWindow" />
  <action id="Resume">
    <keyboard-shortcut first-keystroke="ctrl meta back_slash" />
  </action>
  <action id="ShowIntentionActions">
    <keyboard-shortcut first-keystroke="meta period" />
    <keyboard-shortcut first-keystroke="alt enter" />
    <keyboard-shortcut first-keystroke="ctrl period" />
  </action>
  <action id="Terminal.SwitchFocusToEditor">
    <keyboard-shortcut first-keystroke="ctrl escape" />
  </action>
</keymap>